{
  "name": "tradie-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cp .env.local .env && expo start",
    "android": "cp .env.local .env && expo start --android",
    "ios": "cp .env.local .env && expo start --ios",
    "web": "cp .env.local .env && expo start --web",
    "start:prod": "expo start",
    "android:prod": "expo start --android",
    "ios:prod": "expo start --ios",
    "web:prod": "expo start --web",
    "deploy": "npx expo export -p web && firebase deploy --only hosting,functions",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "deploy:hosting": "npx expo export -p web && firebase deploy --only hosting",
    "deploy:rules": "firebase deploy --only firestore:rules,storage",
    "deploy:all": "npx expo export -p web && firebase deploy"
  },
  "dependencies": {
    "@firebase/app": "^0.14.1",
    "@firebase/auth": "^1.11.0",
    "@firebase/firestore": "^4.9.0",
    "@firebase/functions": "^0.13.0",
    "@firebase/storage": "^0.14.0",
    "@gluestack-ui/config": "^1.1.20",
    "@gluestack-ui/provider": "^0.1.19",
    "@gluestack-ui/themed": "^1.1.73",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "@react-navigation/stack": "^7.4.7",
    "expo": "~53.0.22",
    "expo-av": "^15.1.7",
    "expo-camera": "^16.1.11",
    "expo-constants": "^17.1.7",
    "expo-document-picker": "^13.1.6",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "^7.1.7",
    "expo-media-library": "^17.1.7",
    "expo-notifications": "^0.31.4",
    "expo-router": "^5.1.4",
    "expo-status-bar": "~2.2.3",
    "firebase": "^12.1.0",
    "lucide-react-native": "^0.541.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.15.2",
    "react-native-svg": "^15.12.1",
    "react-native-ui-datepicker": "^3.1.2",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3"
  },
  "private": true
}
